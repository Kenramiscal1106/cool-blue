{
	"name": "Cool Blue",
	"colors": {
		"editor.background": "#00243b",
		"tab.inactiveBackground": "#001e31",
		"tab.border": "#003e64",
		"tab.activeBackground": "#003a5e",
		"editor.foreground": "#fff",
		"activityBarBadge.background": "#00336d",
